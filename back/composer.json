{
  "name": "laravel/laravel",
  "type": "project",
  "description": "Covid",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3",
    "algolia/scout-extended": "^1.20",
    "anourvalar/eloquent-serialize": "^1.1",
    "axlon/laravel-postal-code-validation": "^3.0",
    "doctrine/dbal": "^2.10",
    "facade/ignition": "^2.11",
    "fideloper/proxy": "^4.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "itsgoingd/clockwork": "^5.0",
    "laravel/framework": "^8.0",
    "laravel/horizon": "^5.0",
    "laravel/passport": "^10.0",
    "laravel/slack-notification-channel": "^2.2",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^3.0",
    "lcobucci/jwt": "^3.3",
    "league/flysystem-aws-s3-v3": "^1.0",
    "maatwebsite/excel": "^3.1",
    "predis/predis": "^1.1",
    "propaganistas/laravel-phone": "^4.2",
    "rap2hpoutre/fast-excel": "^2.3",
    "s-ichikawa/laravel-sendgrid-driver": "^2.0",
    "sendinblue/api-v3-sdk": "7.x.x",
    "spatie/geocoder": "^3.5",
    "spatie/laravel-activitylog": "^3.14",
    "spatie/laravel-backup": "^6.7",
    "spatie/laravel-medialibrary": "^8.0.0",
    "spatie/laravel-query-builder": "^2.0",
    "spatie/laravel-ray": "^1.3",
    "spatie/laravel-settings": "^2.3",
    "spatie/laravel-sitemap": "^5.8",
    "spatie/laravel-sluggable": "^2.6",
    "spatie/laravel-tags": "^2.6",
    "staudenmeir/eloquent-has-many-deep": "1.13"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.6",
    "beyondcode/laravel-dump-server": "^1.0",
    "filp/whoops": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.15",
    "fzaninotto/faker": "^1.4",
    "laracasts/generators": "dev-master",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    },
    "paas": {
      "php-config": [
        "short_open_tag=off",
        "memory_limit=1024M"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-install-cmd": [
      "if [ -n \"$OAUTH_PUBLIC_KEY\" ] ; then echo \"$OAUTH_PUBLIC_KEY\" | base64 -d > storage/oauth-public.key ; fi",
      "if [ -n \"$OAUTH_PRIVATE_KEY\" ] ; then echo \"$OAUTH_PRIVATE_KEY\" | base64 -d > storage/oauth-private.key ; fi"
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
