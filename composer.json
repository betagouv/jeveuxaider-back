{
  "name": "laravel/laravel",
  "type": "project",
  "description": "Covid",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "~8.1",
    "ext-sodium": "*",
    "algolia/scout-extended": "^2.0",
    "anourvalar/eloquent-serialize": "^1.1",
    "axlon/laravel-postal-code-validation": "^3.0",
    "doctrine/dbal": "^2.10",
    "guzzlehttp/guzzle": "^7.2",
    "itsgoingd/clockwork": "^5.0",
    "laravel/framework": "^9.2",
    "laravel/horizon": "^5.0",
    "laravel/passport": "^10.0",
    "laravel/scout": "^9.4",
    "laravel/slack-notification-channel": "^2.2",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^3.0",
    "lcobucci/jwt": "^4.1",
    "league/flysystem-aws-s3-v3": "^3.0",
    "maatwebsite/excel": "^3.1",
    "mockery/mockery": "^1.5",
    "predis/predis": "^1.1",
    "propaganistas/laravel-phone": "^4.2",
    "s-ichikawa/laravel-sendgrid-driver": "^2.0",
    "sendinblue/api-v3-sdk": "7.x.x",
    "spatie/geocoder": "^3.5",
    "spatie/laravel-activitylog": "^4.4",
    "spatie/laravel-ignition": "^1.0",
    "spatie/laravel-medialibrary": "^10.0.0",
    "spatie/laravel-query-builder": "^5.0",
    "spatie/laravel-ray": "^1.3",
    "spatie/laravel-settings": "^2.3",
    "spatie/laravel-sitemap": "^6.1",
    "spatie/laravel-sluggable": "^3.3",
    "spatie/laravel-tags": "^4.3",
    "staudenmeir/eloquent-has-many-deep": "1.15",
    "symfony/http-client": "^6.1",
    "symfony/sendinblue-mailer": "^6.1"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.6",
    "beyondcode/laravel-dump-server": "^1.0",
    "filp/whoops": "^2.0",
    "friendsofphp/php-cs-fixer": "^3.6",
    "fzaninotto/faker": "^1.4",
    "laracasts/generators": "dev-master",
    "laravel/pint": "^1.0",
    "nunomaduro/collision": "^6.1",
    "phpunit/phpunit": "^9.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    },
    "paas": {
      "php-config": [
        "short_open_tag=off",
        "memory_limit=2048M",
        "max_execution_time=300"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "pre-install-cmd": [
      "if [ -n \"$OAUTH_PUBLIC_KEY\" ] ; then echo \"$OAUTH_PUBLIC_KEY\" | base64 -d > storage/oauth-public.key ; fi",
      "if [ -n \"$OAUTH_PRIVATE_KEY\" ] ; then echo \"$OAUTH_PRIVATE_KEY\" | base64 -d > storage/oauth-private.key ; fi"
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
