{
    "name": "laravel/laravel",
    "type": "project",
    "description": "Covid",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "algolia/algoliasearch-client-php": "^2.7",
        "algolia/scout-extended": "^1.9",
        "axlon/laravel-postal-code-validation": "^2.1",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.0",
        "jorijn/laravel-security-checker": "^2.0",
        "laravel/framework": "^7.0",
        "laravel/horizon": "^4.3",
        "laravel/passport": "^8.0",
        "laravel/slack-notification-channel": "^2.2",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.1",
        "lcobucci/jwt": "^3.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "maatwebsite/excel": "^3.1",
        "predis/predis": "^1.1",
        "s-ichikawa/laravel-sendgrid-driver": "^2.0",
        "sendinblue/api-v3-sdk": "7.x.x",
        "sentry/sentry-laravel": "^1.7",
        "spatie/geocoder": "^3.5",
        "spatie/laravel-activitylog": "^3.14",
        "spatie/laravel-backup": "^6.7",
        "spatie/laravel-medialibrary": "^8.0.0",
        "spatie/laravel-query-builder": "^2.0",
        "spatie/laravel-sitemap": "^5.8",
        "spatie/laravel-tags": "^2.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.15",
        "fzaninotto/faker": "^1.4",
        "laracasts/generators": "dev-master",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "phpro/grumphp": "^0.19.1",
        "phpunit/phpunit": "^8.5",
        "spatie/laravel-ray": "^1.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "paas": {
            "php-config": [
                "short_open_tag=off"
              ]
          }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-install-cmd": [
            "if [ -n \"$OAUTH_PUBLIC_KEY\" ] ; then echo \"$OAUTH_PUBLIC_KEY\" | base64 -d > storage/oauth-public.key ; fi",
            "if [ -n \"$OAUTH_PRIVATE_KEY\" ] ; then echo \"$OAUTH_PRIVATE_KEY\" | base64 -d > storage/oauth-private.key ; fi"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
